{
  "manifest_version": 2,
  "name": "Soffid ESSO",
  "description": "Soffid Enterprise Single Sign-On",
  "version": "1.0.6",
  "icons": { "32": "icon.png", "128": "soffid.png"},

  "permissions": [
	"*://*/"
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [ {
    "matches": ["*://*/*"],
    "js": ["content.js"]
  } ]
}
