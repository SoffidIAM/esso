{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content.js" ],
      "matches": [ "*://*/*" ]
   } ],
   "description": "Soffid Enterprise Single Sign-On extension (version 3)",
   "icons": {
      "128": "soffid.png",
      "32": "icon.png"
   },
   "manifest_version": 2,
   "name": "Soffid ESSO v3",
   "permissions": [ "*://*/", "tabs", "nativeMessaging" ],
   "version": "3.0.0"
}
